{
  "expo": {
    "name": "Track IO",
    "slug": "track-io",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "trackmyfleetmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCzZUQIwMoVFihzLrCt1BXVH5Y44YdDMpM"
        }
      },
      "edgeToEdgeEnabled": true,
      "package": "com.trackmyfleet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/whitelogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#00a66b"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
    
  }
}